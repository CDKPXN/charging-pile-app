<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kpxn.rongEchong" version="1.1.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>蓉E充</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="bdapp://*/*" />
    <allow-intent href="baidumap://*/*" />
    <allow-intent href="iosamap://*/*" />
    <allow-intent href="androidamap://*/*" />
    <allow-intent href="amapuri://*/*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/100.png" />
        <icon density="mdpi" src="res/icon/android/100.png" />
        <icon density="hdpi" src="res/icon/android/100.png" />
        <icon density="xhdpi" src="res/icon/android/100.png" />
        <icon density="xxhdpi" src="res/icon/android/100.png" />
        <preference name="ShowSplashScreenSpinner" value="false" />
        <preference name="SplashScreenDelay" value="0" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/icon/ios/180.png" width="180" />
        <icon height="60" src="res/icon/ios/60.png" width="60" />
        <icon height="120" src="res/icon/ios/120.png" width="120" />
        <icon height="76" src="res/icon/ios/76.png" width="76" />
        <icon height="152" src="res/icon/ios/152.png" width="152" />
        <icon height="40" src="res/icon/ios/40.png" width="40" />
        <icon height="80" src="res/icon/ios/80.png" width="80" />
        <icon height="57" src="res/icon/ios/57.png" width="57" />
        <icon height="114" src="res/icon/ios/114.png" width="114" />
        <icon height="72" src="res/icon/ios/72.png" width="72" />
        <icon height="144" src="res/icon/ios/144.png" width="144" />
        <icon height="29" src="res/icon/ios/29.png" width="29" />
        <icon height="58" src="res/icon/ios/58.png" width="58" />
        <icon height="50" src="res/icon/ios/50.png" width="50" />
        <icon height="100" src="res/icon/ios/100.png" width="100" />
        <icon height="1024" src="res/icon/ios/1024.png" width="1024" />
        <splash height="480" src="res/icon/ios/launch/320-480.png" width="320" />
        <splash height="960" src="res/icon/ios/launch/640-960.png" width="640" />
        <splash height="1024" src="res/icon/ios/launch/768-1024.png" width="768" />
        <splash height="2048" src="res/icon/ios/launch/1536-2048.png" width="1536" />
        <splash height="768" src="res/icon/ios/launch/1024-768.png" width="1024" />
        <splash height="1536" src="res/icon/ios/launch/2048-1536.png" width="2048" />
        <splash height="1136" src="res/icon/ios/launch/640-1136.png" width="640" />
        <splash height="1334" src="res/icon/ios/launch/750-1334.png" width="750" />
        <splash height="2208" src="res/icon/ios/launch/1242-2208.png" width="1242" />
        <splash height="1242" src="res/icon/ios/launch/2208-1242.png" width="2208" />
        <preference name="ShowSplashScreenSpinner" value="false" />
        <preference name="SplashScreenDelay" value="0" />
        <preference name="FadeSplashScreenDuration" value="0" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-android-permissions" spec="^1.0.0" />
    <plugin name="cordova-plugin-appavailability" spec="^0.4.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-x-toast" spec="^2.7.0" />
    <plugin name="cordova-plugin-cszbar" spec="^1.3.4" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <config-file parent="LSApplicationQueriesSchemes" target="*-Info.plist">
        <array>
            <string>iosamap</string>
            <string>baidumap</string>
        </array>
    </config-file>
    <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
        <string>App需要您的同意,才能在使用期间访问位置用于查找附近充电站</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
        <string>App需要您的同意,才能访问相机用于扫码充电</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>App需要您的同意,才能访问相册用于扫码充电</string>
    </edit-config>
    <plugin name="cordova-plugin-wechat" spec="^2.1.0">
        <variable name="WECHATAPPID" value="wxacf2748e65b13188" />
    </plugin>
    <plugin name="cordova-plugin-qqsdk" spec="https://github.com/zzhDmon/cordova_qq.git">
        <variable name="QQ_APP_ID" value="1107765781" />
    </plugin>
    <engine name="android" spec="^7.1.4" />
</widget>
